{
    "name": "chrome-extension",
    "version": "1.2.0",
    "main": "index.ts",
    "license": "MIT",
    "scripts": {
        "dev:chrome": "webpack --watch --config webpack.dev.ts",
        "build:chrome": "webpack --config webpack.prod.ts",
        "format": "prettier --write --loglevel silent .",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "correct": "npm run format && npm run lint",
        "package": "npm run build:prod && zip -r packages/$(date -u +\"%Y-%m-%dT%H:%M:%SZ\").zip build"
    },
