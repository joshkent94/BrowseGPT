        "@mui/material": "^5.13.2",
        "@tanstack/react-query": "^4.29.7",
        "@trpc/client": "^10.28.0",
        "@trpc/react-query": "^10.28.0",
        "@trpc/server": "^10.28.0",
        "@types/chrome": "^0.0.242",
