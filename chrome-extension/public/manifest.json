{
    "manifest_version": 3,
    "name": "BrowseGPT",
    "description": "An AI assistant for your browser",
    "version": "1.2.0",
    "action": {
        "default_title": "Click to open panel"
    },
    "icons": {
        "16": "icon-with-background.png",
        "48": "icon-with-background.png",
        "128": "icon-with-background.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "permissions": [
        "sidePanel",
        "tabs",
        "geolocation",
        "identity",
        "identity.email",
        "cookies"
    ],
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+Space",
                "mac": "Command+Shift+Space"
            }
        }
    },
    "key": "",
    "oauth2": {
        "client_id": "20795077364-ag77isp97pa25ag0cfl5pp6pudqjvrvr.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/userinfo.profile"]
    }
}
