{
    "name": "browsegpt-server",
    "version": "1.0.0",
    "main": "server.ts",
    "license": "MIT",
    "scripts": {
        "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
        "build": "tsc",
        "dev": "tsc && nodemon -q",
        "format": "prettier --write --loglevel silent .",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "correct": "npm run format && npm run lint"
    },
    "dependencies": {
        "@trpc/client": "^10.28.0",
        "@trpc/server": "^10.28.0",
        "@types/crypto-js": "^4.1.1",
        "@types/express": "^4.17.17",
        "@types/node": "^20.2.3",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "crypto-js": "^4.1.1",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "nodemon": "^2.0.22",
        "openai": "^3.2.1",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3",
        "zod": "^3.21.4"
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules"
        ],
        "watch": [
            "routes",
            "utils",
            "server.ts"
        ],
        "exec": "node -r tsconfig-paths/register -r ts-node/register ./server.ts",
        "ext": "ts, js"
    }
}
