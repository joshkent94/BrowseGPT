{
    "name": "server",
    "version": "1.0.0",
    "main": "server.ts",
    "license": "MIT",
    "scripts": {
        "start": "tsc && node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
        "build": "tsc",
        "dev": "nodemon -q",
        "format": "prettier --write --loglevel silent .",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "correct": "npm run format && npm run lint",
        "prisma:migrate:dev": "npx prisma migrate dev && npx prisma format",
        "prisma:migrate:deploy": "npx prisma migrate deploy"
    },
    "dependencies": {
        "@prisma/client": "^4.16.2",
        "@trpc/client": "^10.28.0",
        "@trpc/server": "^10.28.0",
        "@types/connect-pg-simple": "^7.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/node": "^20.2.3",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "axios": "^1.4.0",
        "connect-pg-simple": "^9.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "nodemon": "^2.0.22",
        "openai": "^3.2.1",
        "prettier": "^2.8.8",
        "prisma": "^4.16.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3",
        "zod": "^3.21.4"
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules"
        ],
        "watch": [
            "routes",
            "utils",
            "server.ts",
            "../index.d.ts"
        ],
        "exec": "npm start",
        "ext": "ts, js"
    }
}
