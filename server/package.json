{
    "name": "browsegpt-server",
    "version": "0.0.1",
    "description": "Backend for the BrowseGPT extension",
    "main": "index.ts",
    "scripts": {
        "start": "node dist/server.js",
        "build": "tsc",
        "dev": "concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
        "format": "prettier --write --loglevel silent .",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "correct": "npm run format && npm run lint"
    },
    "author": "Joshua Kent",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.2.3",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "concurrently": "^8.0.1",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "openai": "^3.2.1"
    }
}
