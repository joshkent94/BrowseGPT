{
	"$schema": "https://turbo.build/schema.json",
    "pipeline": {
		"dev": {
			"persistent": true,
			"cache": false,
			"outputMode": "none"
		},
		"correct": {
			"cache": false
		},
		"extension#build": {
			"dependsOn": ["server#build"],
			"cache": false
		},
		"server#build": {
			"cache": false
		},
		"build:prod": {
			"dependsOn": ["server#build"],
			"cache": false
		},
		"package": {
			"dependsOn": ["server#build"],
			"cache": false
		},
		"prisma:migrate:dev": {
			"cache": false
		}
	}
}
