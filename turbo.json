{
	"$schema": "https://turbo.build/schema.json",
    "pipeline": {
		"dev:chrome": {
			"persistent": true,
			"dependsOn": ["server#build", "prisma:migrate:dev"],
			"cache": false,
			"outputMode": "none"
		},
		"dev:edge": {
			"persistent": true,
			"dependsOn": ["server#build", "prisma:migrate:dev"],
			"cache": false,
			"outputMode": "none"
		},
		"build:chrome": {
			"dependsOn": ["server#build"],
			"cache": false
		},
		"build:edge": {
			"dependsOn": ["server#build"],
			"cache": false
		},
		"package:chrome": {
			"dependsOn": ["server#build"],
			"cache": false
		},
		"package:edge": {
			"dependsOn": ["server#build"],
			"cache": false
		},
		"server#build": {
			"cache": false
		},
		"correct": {
			"cache": false
		},
		"prisma:migrate:dev": {
			"cache": false
		}
	}
}
